// While Loops
// Sum of first 10 numbers
d = 0
j = 1

while j<= 10
    d = d+j
    j = j+1
end

disp(d, "Sum of first 10 natural numbers ")

// Sum of first 10 even number excluding 0
d = 0
i = 2

while i <= 20
    d = d+i
    i = i+2
end
disp(d, 'Sum of first 10 even numbers ')

// Sum of first 10 odd numbers
s = 0
k = 1

while k <= 20
    s = s+k
    k = k+2
end
disp(s, "Sum of first 10 odd numbers ")
