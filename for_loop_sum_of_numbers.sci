// For Loops 
// Sum of first odd numbers from 1 - 20
s = 0
disp("Odd numbers till 20 are ")
for i=1:2:20
    disp(i)
    s = s+i
end
disp(s, "sum of odd numbers = ")

// Sum of first even numbers till 20
s = 0
disp("Even numbers till 20 are ")
for i = 0:2:20
    disp(i)
    s = s+i
end
disp(s, "sum of even numbers = ")
